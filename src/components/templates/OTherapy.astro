<section class="otherapy-sixth pb-5 mt-sm read-aloud">
    <div class="container ps-5 pe-5">
        <div class="row">
            <div class="col-lg-8 col-sm-12 d-flex flex-column align-items-start justify-content-center pt-5 pt-lg-0">
                {otherapydata.acf.sixth_contents && (
                    <div class="col-12 mb-0 mb-md-3 pe-lg-5 text-black" set:html={otherapydata.acf.sixth_contents} />
                )}
            </div>
            <div class="col-lg-4 col-sm-12 d-flex flex-column align-items-center justify-content-start">
                {otherapydata.acf.sixth_section_image && (
                <img 
                    class="rounded-5" 
                    src={otherapydata.acf.sixth_section_image.url} 
                    alt={otherapydata.acf.sixth_section_image.alt}
                    width={otherapydata.acf.sixth_section_image.width}
                    height={otherapydata.acf.sixth_section_image.height}
                />
                )}
            </div>    


        </div>
    </div>
</section>

<section class="otherapy-seventh pt-50 pb-50 mt-sm read-aloud">
    <div class="container ps-5 pe-5">
        <div class="row">
            
            <div class="col-lg-6 col-sm-12 d-flex flex-column align-items-start justify-content-center">
                {otherapydata.acf.seventh_section_titleheading && (
                    <h2 class="text-white" set:html={otherapydata.acf.seventh_section_titleheading} />
                )}
                {otherapydata.acf.seventh_contents_col_1 && (
                    <div class="card">
                        <div class="card-body">
                            <div class="card-text text-black" set:html={otherapydata.acf.seventh_contents_col_1} />

                            <ul class="list-group">
                            {otherapydata.acf.featured_list_seventh_1 && otherapydata.acf.featured_list_seventh_1.map((featuredItem6, index) => (
                                <li key={index}
                                class="list-group-item">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="#dc3545" class="bi bi-heart-fill" viewBox="0 0 16 16">
  <path fill-rule="evenodd" d="M8 1.314C12.438-3.248 23.534 4.735 8 15-7.534 4.736 3.562-3.248 8 1.314"/>
</svg> {featuredItem6.featured_item} </li>
                            ))}
                        </ul>
                        </div>
                    </div>
                )}
            </div>
            <div class="col-lg-6 col-sm-12 d-flex flex-column align-items-start justify-content-center">
                {otherapydata.acf.seventh_contents_col_2 && (
                    <h2 class="text-white" set:html={otherapydata.acf.seventh_contents_col_2} />
                    <div class="card">
                        <div class="card-body">
                            <ul class="list-group">
                            {otherapydata.acf.featured_list_seventh_2 && otherapydata.acf.featured_list_seventh_2.map((featuredItem7, index) => (
                                <li key={index}
                                class="list-group-item">
                                <div style="display: flex; align-items: center;">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="#009345" class="bi bi-check-circle-fill" viewBox="0 0 16 16">
                <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0m-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z"/>
              </svg><span style="flex:1;margin-left: 10px;">{featuredItem7.featured_item}</span></div></li>
                            ))}
                        </ul>
                        {otherapydata.acf.seventh_contents_col_3 && (
                            <div class="text-black" set:html={otherapydata.acf.seventh_contents_col_3} />
                        )}
                        </div>
                    </div>
                )}
            </div>
        
        </div>
    </div>
</section>