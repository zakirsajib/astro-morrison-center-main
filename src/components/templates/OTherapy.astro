---
const { otherapydata } = Astro.props;

---

<style is:inline>
    .otherapy-seventh {
        background-color: #413C86;
    }
    .card {
        background: linear-gradient(to bottom, #ffffff, #eeeded);
        background: -webkit-linear-gradient(top, #ffffff, #eeeded);
        background: -moz-linear-gradient(top, #ffffff, #eeeded);
        background: -ms-linear-gradient(top, #ffffff, #eeeded);
        background: -o-linear-gradient(top, #ffffff, #eeeded);
        padding: 20px;
        border-radius: 30px;
        box-shadow: 5px 5px 15px rgba(0, 0, 0, 0.15);
        margin-bottom: 20px;
        transition: all 0.3s ease-in-out;
        cursor: text;
        height: 100%;
    }
    .card h3 {
        padding-bottom: 20px;
    }
    .card-text {
        font-size: 18px;
    }
    .card-text p {
        margin-bottom: 0;
    }
    .card-text ul {
        padding-left: 2rem;
        padding-top: 2rem;
    }
    .card-text ul li {
        list-style-type: disc;
        line-height: 20px;
        padding-bottom: 20px;
    }
    .otherapy-seventh .card-body a,
    .card-text a {
        border-bottom: 2px solid #009345;
    }
    .otherapy-seventh .list-group-item {
        background-color: transparent;
    }
    .otherapy-seventh .card-body ul {
        padding-bottom: 1rem;
        padding-top: 1rem;
        padding-left: 0rem;
    }
    
</style>

<section class="otherapy-hero pb-5 mt-md read-aloud">
    <div class="container ps-5 pe-5">
      <div class="row">
        <div class="col-lg-7 col-sm-12 d-flex flex-column align-items-start justify-content-center">
            <div class="pe-6 text-black" set:html={otherapydata.acf.hero_contents} />

            <div class="text-black" set:html={otherapydata.acf.second_contents} />

        {otherapydata.acf.multiple_column === 'Yes' ? (
                <div class="row py-1 w-100">
                    <div class="col-12 col-md-6 col-lg-6 featList">
                        <ul class="list-group">
                            {otherapydata.acf.featured_list && otherapydata.acf.featured_list.map((featuredItem, index) => (
                                <li key={index}
                                class="list-group-item">
                                 {featuredItem.featured_item} </li>
                            ))}
                        </ul>
                    </div>
                    <div class="col-12 col-md-6 col-lg-6 featList">
		                <ul class="list-group">
                            {otherapydata.acf.featured_list_2 && otherapydata.acf.featured_list_2.map((featuredItem2, index) => (
                                <li key={index}
                                class="list-group-item">
                                 {featuredItem2.featured_item} </li>
                            ))}
                        </ul>
                    </div>
                </div>

                ) : (
                <div class="row py-1">
                    <div class="col-12 featList">
                        <ul class="list-group">
                            {otherapydata.acf.featured_list && otherapydata.acf.featured_list.map((featuredItem, index) => (
                                <li key={index}
                                class="list-group-item">
                                 {featuredItem.featured_item} </li>
                            ))}
                        </ul>
                    </div>
                </div>
        )}    


        </div>
        <div class="col-lg-5 col-sm-12 d-flex flex-column align-items-center justify-content-start pt-5 pt-lg-0">
          {otherapydata.acf.hero_image && (
            <img 
                class="rounded-5" 
                src={otherapydata.acf.hero_image.url} 
                alt={otherapydata.acf.hero_image.alt}
                width={otherapydata.acf.hero_image.width}
                height={otherapydata.acf.hero_image.height}
            />
          )}
          {otherapydata.acf.hero_video && (
            <div 
                class="slideRight" 
                set:html={otherapydata.acf.hero_video} 
            /> 
        )}
        </div>
      </div>
    </div>
</section>

<section class="otherapy-third pb-5 mt-sm read-aloud">
    <div class="container ps-5 pe-5">
      <div class="row">
        
        <div class="col-lg-6 col-sm-12 d-flex flex-column align-items-center justify-content-center">
          {otherapydata.acf.third_section_image && (
            <img 
                class="rounded-5" 
                src={otherapydata.acf.third_section_image.url} 
                alt={otherapydata.acf.third_section_image.alt}
                width={otherapydata.acf.third_section_image.width}
                height={otherapydata.acf.third_section_image.height}
            />
          )}
        </div>
        <div class="col-lg-6 col-sm-12 d-flex flex-column align-items-start justify-content-center pt-5 pt-lg-0">
            <div class="ps-0 ps-lg-5 text-black" set:html={otherapydata.acf.third_contents} />

            {otherapydata.acf.multiple_column_2 === 'Yes' ? (
                <div class="row py-3 ps-0 ps-lg-5">
                    <div class="col-12 col-md-6 col-lg-6 featList">
                        <ul class="list-group">
                            {otherapydata.acf.featured_list_third && otherapydata.acf.featured_list_third.map((featuredItem, index) => (
                                <li key={index}
                                class="list-group-item">
                                <div style="display: flex; align-items: center;">
                                <span style="flex:1;margin-left:10px;">{featuredItem.featured_item}</span></div></li>
                            ))}
                        </ul>
                    </div>
                    <div class="col-12 col-md-6 col-lg-6 featList">
		                <ul class="list-group">
                            {otherapydata.acf.featured_list_3 && otherapydata.acf.featured_list_3.map((featuredItem3, index) => (
                                <li key={index}
                                class="list-group-item">
                                <div style="display: flex; align-items: center;">
                                <span style="flex:1;margin-left: 10px;">{featuredItem3.featured_item}</span></div></li>
                            ))}
                        </ul>
                    </div>
                </div>

                ) : (
                <div class="row ps-0 ps-lg-5 py-3">
                    <div class="col-12 featList">
                        <ul class="list-group">
                            {otherapydata.acf.featured_list_third && otherapydata.acf.featured_list_third.map((featuredItem4, index) => (
                                <li key={index}
                                class="list-group-item">
                                 {featuredItem4.featured_item} </li>
                            ))}
                        </ul>
                    </div>
                </div>
        )} 

        {otherapydata.acf.third_contents_extra && (
            <div class="ps-0 ps-lg-5" set:html={otherapydata.acf.third_contents_extra} />
        )}
        </div>
      </div>
    </div>
</section>

